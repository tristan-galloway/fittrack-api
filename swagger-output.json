              "items": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string",
                    "example": "fit_jane"
                  },
                  "email": {
                    "type": "string",
                    "example": "jane@example.com"
                  },
                  "avatarUrl": {
                    "type": "string",
                    "example": "https://example.com/avatar/jane.jpg"
                  },
                  "fitnessGoals": {
