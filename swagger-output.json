    "/workouts/": {
      "get": {
        "description": "Get all workouts",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of workouts",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "userId": { "type": "string", "example": "6830e84aed56c2256c29b10a" },
                  "date": { "type": "string", "example": "2025-05-21T07:30:00Z" },
                  "title": { "type": "string", "example": "Morning Full Body Circuit" },
                  "exercises": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": { "type": "string", "example": "Push-ups" },
                        "sets": { "type": "integer", "example": 3 },
                        "reps": { "type": "integer", "example": 12 },
                        "weight": { "type": "number", "example": 0 }
                      }
                    }
                  },
                  "duration": { "type": "integer", "example": 40 },
                  "intensity": { "type": "string", "example": "medium" },
                  "notes": { "type": "string", "example": "Felt energized today, good start to the week!" }
                }
              }
            }
          },
